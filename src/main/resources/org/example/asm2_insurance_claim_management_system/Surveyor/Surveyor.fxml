    <children>
        <Label text="Surveyor Page">
            <padding>
                <Insets bottom="10.0" left="5.0" right="5.0" top="5.0" />
            </padding>
            <VBox.margin>
                <Insets left="14.0" />
            </VBox.margin>
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
        </Label>
      <Button mnemonicParsing="false" text="Require More Information On A Claim" />
        <Button fx:id="ProposeClaimFormButton" onAction="#ProposeClaimForm" mnemonicParsing="false" text="Propose a Claim">
            <VBox.margin>
                <Insets left="28.0" />
            </VBox.margin>
        </Button>
    </children>
