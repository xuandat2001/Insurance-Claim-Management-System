<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="201.0" prefWidth="418.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.asm2_insurance_claim_management_system.SurveyorGUI.FeaturesSurveyor">
    <children>
        <Label alignment="CENTER" prefHeight="22.0" prefWidth="515.0" text="Require More Information On A Claim" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="15.0" />
         </font></Label>
      <Label text="Enter ClaimID to download document from">
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </Label>
      <TextField fx:id="textClaimIdForDocument" promptText="ClaimID">
         <VBox.margin>
            <Insets />
         </VBox.margin></TextField>
      <HBox prefHeight="3.0" prefWidth="482.0" spacing="225.0">
         <children>
              <Button mnemonicParsing="false" onAction="#downloadDocument" text="Download Document" />
              <Button mnemonicParsing="false" onAction="#goBack" text=" Back" />
         </children>
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </HBox>
    </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
