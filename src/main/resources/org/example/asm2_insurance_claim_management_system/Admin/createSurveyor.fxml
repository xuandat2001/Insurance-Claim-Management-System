<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.asm2_insurance_claim_management_system.AdminGUI.CRUDSurveyorController">
    <padding>
        <Insets bottom="10" left="10" right="10" top="10" />
    </padding>
   <Label alignment="CENTER" prefHeight="21.0" prefWidth="194.0" text="Create Surveyor Account" textAlignment="CENTER">
      <font>
         <Font name="System Bold" size="15.0" />
      </font>
   </Label>
    <Label text="Set the Username (ID)">
      <VBox.margin>
         <Insets top="10.0" />
      </VBox.margin></Label>
    <TextField fx:id="textFieldId" promptText="Username">
      <VBox.margin>
         <Insets />
      </VBox.margin></TextField>
    <Label text="Set password">
      <VBox.margin>
         <Insets top="10.0" />
      </VBox.margin></Label>
    <TextField fx:id="textFieldPassword" promptText="Password">
      <VBox.margin>
         <Insets />
      </VBox.margin></TextField>
    <Label text="Set Full Name">
      <VBox.margin>
         <Insets top="10.0" />
      </VBox.margin></Label>
    <TextField fx:id="textFieldFullName" promptText="Full Name">
      <VBox.margin>
         <Insets />
      </VBox.margin></TextField>
   <HBox prefHeight="0.0" prefWidth="200.0" spacing="105.0">
      <children>
          <Button onAction="#goBack" text="Back" />
          <Button mnemonicParsing="false" onAction="#createEntity" text="Create" />
      </children>
      <VBox.margin>
         <Insets top="10.0" />
      </VBox.margin>
   </HBox>
</VBox>
