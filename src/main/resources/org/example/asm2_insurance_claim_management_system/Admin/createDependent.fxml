<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.asm2_insurance_claim_management_system.AdminGUI.CRUDDependentController">
    <padding>
        <Insets bottom="10" left="10" right="10" top="10" />
    </padding>
    <Label text="Set your ID(username)" />
    <TextField fx:id="textFieldId" />
    <Label text="Set your password" />
    <TextField fx:id="textFieldPassword" />
    <Label text="Set your FullName" />
    <TextField fx:id="textFieldFullName" />
    <Label  text="PolicyHolder ID" />
    <TextField fx:id="textFieldPolicyHolder" />
    <Label text="Policy OwnerID" />
    <TextField fx:id="textFieldPolicyOwner" />
    <Button mnemonicParsing="false" onAction="#createEntity" text="Create" />
</VBox>
