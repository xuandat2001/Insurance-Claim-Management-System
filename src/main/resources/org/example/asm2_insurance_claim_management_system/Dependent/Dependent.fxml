<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.asm2_insurance_claim_management_system.DependentGUI.DependentController">
    <children>
        <Label text="Dependent page">
            <font>
                <Font name="System Bold" size="15.0" />
            </font>
            <VBox.margin>
                <Insets left="10.0" top="10.0" />
            </VBox.margin>
        </Label>
        <Button fx:id="showInfo" mnemonicParsing="false" onAction="#showInfo" text="View Your Information">
            <VBox.margin>
                <Insets left="10.0" right="10.0" top="10.0" />
            </VBox.margin>
        </Button>
        <Button fx:id="retrieveClaim" mnemonicParsing="false" onAction="#retrieveClaim" text="Retrieve A Claim">
            <VBox.margin>
                <Insets left="10.0" right="10.0" top="10.0" />
            </VBox.margin>
        </Button>
        <Button mnemonicParsing="false" onAction="#goBack" text="Back">
            <VBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </VBox.margin>
        </Button>

    </children>
</VBox>
