<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="123.0" prefWidth="149.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.asm2_insurance_claim_management_system.ManagerGUI.ManagerController">
    <children>
        <Label alignment="TOP_LEFT" prefHeight="25.0" prefWidth="115.0" text="Manager Page">
            <font>
                <Font name="System Bold" size="15.0" />
            </font>
            <VBox.margin>
                <Insets left="10.0" top="10.0" />
            </VBox.margin>
        </Label>
        <Button fx:id="ApproveClaimFormButton" mnemonicParsing="false" onAction="#approveClaimForm" text="Approve a Claim">
            <VBox.margin>
                <Insets left="10" top="10.0" />
            </VBox.margin>
        </Button>
        <Button fx:id="RejectClaimFormButton" mnemonicParsing="false" onAction="#rejectClaimForm" text="Reject a Claim">
            <VBox.margin>
                <Insets left="10" top="10.0" />
            </VBox.margin>
        </Button>
    </children>
</VBox>
